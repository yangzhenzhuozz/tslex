{
  "compilerOptions": {
    "target": "ESNext",
    "module": "nodenext",
    "declaration": true,
    "sourceMap": false,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "nodenext",
    "outDir": "./dist"
  },
  "include": ["src/**/*"],
  "exclude": ["dist"]
}
